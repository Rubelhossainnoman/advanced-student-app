<div class="section my-5">
    <div class="advanced_studen_app">
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-lg-12 col-md-10 col-sm-10">
                    <div class="start">
                        <div class="card shadow-lg">
                            <div class="card-header py-3">
                                <div class="d-flex justify-content-between">
                                    <a href="/student" class="btn btn-primary"><i class="fas fa-arrow-left"></i> Back to Home</a>
                                    <a style="color:white;" href="/student/unverify" class="btn btn-info request_button" style="margin-left:8px;"><i class="fas fa-info-circle"></i> Please verify your Email and your Phone</a>
                                </div>
                            </div>
                            <div class="card-body pt-4 px-4">
                                <div class="unverify_student">
                                    <table class="table table-striped" id="unverifyTable">
                                      <thead>
                                        <tr>
                                          <th>#</th>
                                          <th>Image</th>
                                          <th>Name</th>
                                          <th>Email</th>
                                          <th>Phone</th>
                                          <th>Department</th>
                                          <th>Status</th>
                                          <th>Action</th>
                                        </tr>
                                      </thead>
                                      <tbody class="align-middle unverify_data">
                                        <% unverifyed_student.forEach(data => { %>
                                          <tr>
                                            <td><%= data.id %> </td>
                                            <td>
                                              <% if (data.photo == "avatar.jpg") { %>
                                                <img src="/images/<%= data.photo %> " alt="">
                                                <% } else { %>
                                                  <img src="/images/student/<%= data.photo %> " alt="">
                                              <% } %>
                                            </td>
                                            <td><%= data.name %> </td>
                                            <td><%= data.email %> </td>
                                            <td><%= data.phone %> </td>
                                            <td><%= data.dep %> </td>
                                            <td><a title="Click To Verify Your Data" href="/student/verify-by-phone/<%= data.id %> " class="btn btn-warning">verify with <i class="fas fa-phone-volume"></i></a></td>
                                            <td>
                                              <a href="/student/delete_student/<%= data.id %>" class="btn btn-danger delete_student"><i class="fas fa-trash"></i> Delete</a>
                                            </td>
                                          </tr>
                                        <% }) %>
